<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <!--设置背景色-->
            <solid android:color="#000000" />
            <!--设置圆角-->
            <corners android:radius="15dp" />
            <!--设置边框线的宽度和颜色-->
            <stroke android:width="1dp" android:color="#4AE6C1" />
        </shape>
    </item>
</layer-list>